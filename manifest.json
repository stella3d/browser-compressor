{
  "name": "Audio Dynamics Compressor",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Smooth out uneven audio volumes in media,  make loud parts of media quieter.",
  "homepage_url": "https:/github.com/stella3d/browser-compressor",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_ui": {
    "page" : "src/options/index.html",
    "open_in_tab": true
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "Audio Compressor Controls",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [ "activeTab" ]
}