{
  "name": "Audio Dynamics Compressor",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Smooth out uneven audio volumes in media",
  "homepage_url": "https:/github.com/stella3d/browser-compressor",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "options_page": "src/options/index.html",
  "page_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "Audio Dynamics Compressor",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://youtube.com/watch*",
        "https://www.youtube.com/watch*",
        "https://www.twitch.tv/*",
        "https://twitter.com/*",
        "https://www.pscp.tv/*",
        "https://www.facebook.com/*"
        ,
        "https://amazon.com/gp/video*",
        "https://www.amazon.com/gp/video*",
        "https://www.amazon.com.au/gp/video*",
        "https://www.amazon.com.mx/gp/video*",
        "https://www.amazon.ca/gp/video*",
        "https://www.amazon.eu/gp/video*",
        "https://www.amazon.co.jp/gp/video*"
        ,
        "https://cbs.com/*",
        "https://vimeo.com/*",
        "https://www.mixcloud.com/*"
        ,
        "http://*:*/*.mp3",
        "http://*:*/*.mp4",
        "http://*:*/*.ogg",
        "http://*:*/*.webm",
        "https://*:*/*.mp3",
        "https://*:*/*.mp4",
        "https://*:*/*.ogg",
        "https://*:*/*.webm"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}